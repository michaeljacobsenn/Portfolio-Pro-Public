<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CAPACITOR_DEBUG</key>
	<string>$(CAPACITOR_DEBUG)</string>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleDisplayName</key>
	<string>Catalyst Cash</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(MARKETING_VERSION)</string>

	<key>CFBundleVersion</key>
	<string>$(CURRENT_PROJECT_VERSION)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
		<key>NSAllowsArbitraryLoadsInWebContent</key>
		<true/>
	</dict>
	<key>NSFaceIDUsageDescription</key>
	<string>Catalyst Cash requires Face ID to securely authenticate your financial data.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Catalyst Cash needs access to save exported run reports to your device.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Catalyst Cash needs access to share exported run reports from your device.</string>
	<key>NSCalendarsUsageDescription</key>
	<string>Catalyst Cash accesses your calendar to schedule financial reminders.</string>
	<key>NSContactsUsageDescription</key>
	<string>Catalyst Cash accesses contacts if you choose to share your reports.</string>
	<key>NSCameraUsageDescription</key>
	<string>Catalyst Cash needs camera access for reading barcodes or cards.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Catalyst Cash needs microphone access for voice input features if enabled.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Catalyst Cash accesses your location to map local transactions.</string>
	<key>NSUserTrackingUsageDescription</key>
	<string>Catalyst Cash uses tracking to securely attribute subscription data.</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>fetch</string>
		<string>processing</string>
	</array>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
	</array>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>com.jacobsen.portfoliopro.sync</string>
	</array>
	<key>UIStatusBarStyle</key>
	<string>UIStatusBarStyleLightContent</string>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<true/>
</dict>
</plist>
